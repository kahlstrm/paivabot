datasource db {
  // could be postgresql or mysql
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator db {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

model WeatherIcon {
  id         String   @id @default(cuid())
  code      Int @unique
  temperature Int @unique
  votes_yes    Int @default(1)
  votes_no Int  @default(1)
}